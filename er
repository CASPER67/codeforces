#include <bits/stdc++.h>

using namespace std;

int main()
{
    ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    long long t;
    cin>>t;
    while(t--)
    {
        long long a[3],i;
        for(i=0;i<3;i++)
        {
            cin>>a[i];
        }
        sort(a,a+3);
        if(a[2]<=a[1]+a[0]+1)
        {
            cout<<"Yes"<<endl;
        }
        else
            cout<<"No"<<endl;
    }
    return 0;
}
